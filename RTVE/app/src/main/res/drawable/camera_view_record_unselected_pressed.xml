<?xml version="1.0" encoding="utf-8"?>
<layer-list
   xmlns:android="http://schemas.android.com/apk/res/android">
   <item>
      <shape xmlns:android="http://schemas.android.com/apk/res/android"
             android:shape="rectangle">
         <solid android:color="#000000"/>
         <size
            android:width="50dp"
